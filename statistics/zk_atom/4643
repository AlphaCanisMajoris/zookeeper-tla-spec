STATE_16 == 
/\ connectInfo = ( s2 :> [sid |-> s0, syncMode |-> NONE, nlRcv |-> FALSE] @@
  s1 :> [nlRcv |-> FALSE, syncMode |-> NONE, sid |-> NullPoint] @@
  s0 :> [nlRcv |-> FALSE, syncMode |-> NONE, sid |-> NullPoint] )
/\ forwarding = (s2 :> {} @@ s1 :> {} @@ s0 :> {})
/\ history = ( s2 :>
      << [ zxid |-> <<1, 1>>,
           value |-> 101,
           ackSid |-> {s2, s1, s0},
           epoch |-> 1 ],
         [ zxid |-> <<1, 2>>,
           value |-> 102,
           ackSid |-> {s2, s1},
           epoch |-> 1 ] >> @@
  s1 :>
      << [zxid |-> <<1, 1>>, value |-> 101, ackSid |-> {s1, s0}, epoch |-> 1],
         [zxid |-> <<1, 2>>, value |-> 102, ackSid |-> {s1}, epoch |-> 1] >> @@
  s0 :> <<[zxid |-> <<1, 1>>, value |-> 101, ackSid |-> {s0}, epoch |-> 1]>> )
/\ electing = ( s2 :>
      { [sid |-> s2, peerLastZxid |-> <<-1, -1>>, inQuorum |-> TRUE],
        [sid |-> s1, peerLastZxid |-> <<-1, -1>>, inQuorum |-> TRUE],
        [sid |-> s0, peerLastZxid |-> <<-1, -1>>, inQuorum |-> TRUE] } @@
  s1 :>
      { [sid |-> s1, peerLastZxid |-> <<-1, -1>>, inQuorum |-> TRUE],
        [sid |-> s0, peerLastZxid |-> <<-1, -1>>, inQuorum |-> TRUE] } @@
  s0 :>
      { [sid |-> s2, peerLastZxid |-> <<1, 2>>, inQuorum |-> TRUE],
        [sid |-> s0, peerLastZxid |-> <<-1, -1>>, inQuorum |-> TRUE] } )
/\ zabState = (s2 :> SYNCHRONIZATION @@ s1 :> ELECTION @@ s0 :> SYNCHRONIZATION)
/\ aaInv = [ leadership |-> TRUE,
  integrity |-> TRUE,
  agreement |-> TRUE,
  totalOrder |-> TRUE,
  localPrimaryOrder |-> TRUE,
  globalPrimaryOrder |-> TRUE,
  primaryIntegrity |-> TRUE,
  commitCompleteness |-> TRUE,
  commitConsistency |-> TRUE,
  historyConsistency |-> TRUE ]
/\ lastProcessed = ( s2 :> [index |-> 2, zxid |-> <<1, 2>>] @@
  s1 :> [index |-> 2, zxid |-> <<1, 2>>] @@
  s0 :> [index |-> 1, zxid |-> <<1, 1>>] )
/\ rcvBuffer = ( s2 :> (s2 :> <<>> @@ s1 :> <<>> @@ s0 :> <<>>) @@
  s1 :> (s2 :> <<>> @@ s1 :> <<>> @@ s0 :> <<>>) @@
  s0 :> (s2 :> <<>> @@ s1 :> <<>> @@ s0 :> <<>>) )
/\ initialHistory = ( s2 :>
      << [ zxid |-> <<1, 1>>,
           value |-> 101,
           ackSid |-> {s2, s1, s0},
           epoch |-> 1 ],
         [ zxid |-> <<1, 2>>,
           value |-> 102,
           ackSid |-> {s2, s1},
           epoch |-> 1 ] >> @@
  s1 :>
      << [zxid |-> <<1, 1>>, value |-> 101, ackSid |-> {s1, s0}, epoch |-> 1],
         [zxid |-> <<1, 2>>, value |-> 102, ackSid |-> {s1}, epoch |-> 1] >> @@
  s0 :> <<[zxid |-> <<1, 1>>, value |-> 101, ackSid |-> {s0}, epoch |-> 1]>> )
/\ recorder = [ step |-> 15,
  nPartition |-> 0,
  nConsequentFailure |-> 0,
  noExecute |-> {<<"NodeCrash", s1>>},
  nTimeout |-> 2,
  nCrash |-> 3,
  nTransaction |-> 2,
  maxEpoch |-> 3,
  pc |->
      << "ElectionAndDiscovery",
         s0,
         {s2},
         2,
         [index |-> 1, zxid |-> <<1, 1>>],
         3 >>,
  nClientRequest |-> 2 ]
/\ acceptedEpoch = (s2 :> 3 @@ s1 :> 2 @@ s0 :> 3)
/\ connecting = ( s2 :>
      { [sid |-> s2, connected |-> TRUE],
        [sid |-> s1, connected |-> TRUE],
        [sid |-> s0, connected |-> TRUE] } @@
  s1 :> {[sid |-> s1, connected |-> TRUE], [sid |-> s0, connected |-> TRUE]} @@
  s0 :> {[sid |-> s2, connected |-> TRUE], [sid |-> s0, connected |-> TRUE]} )
/\ proposalMsgsLog = { [zxid |-> <<1, 1>>, epoch |-> 1, source |-> s2, data |-> 101],
  [zxid |-> <<1, 2>>, epoch |-> 1, source |-> s2, data |-> 102],
  [zxid |-> <<1, 2>>, epoch |-> 2, source |-> s1, data |-> 102] }
/\ hzxid = (s2 :> <<1, 2>> @@ s1 :> <<1, 2>> @@ s0 :> <<3, 0>>)
/\ lastCommitted = ( s2 :> [index |-> 0, zxid |-> <<0, 0>>] @@
  s1 :> [index |-> 0, zxid |-> <<0, 0>>] @@
  s0 :> [index |-> 0, zxid |-> <<0, 0>>] )
/\ daInv = [ messageLegal |-> TRUE,
  commitConsistent |-> TRUE,
  stateConsistent |-> TRUE,
  proposalConsistent |-> TRUE,
  ackConsistent |-> TRUE ]
/\ currentEpoch = (s2 :> 1 @@ s1 :> 2 @@ s0 :> 3)
/\ learners = (s2 :> {} @@ s1 :> {} @@ s0 :> {s2, s0})
/\ tempMaxEpoch = (s2 :> 1 @@ s1 :> 2 @@ s0 :> 3)
/\ leaderOracle = s0
/\ lastSnapshot = ( s2 :> [zxid |-> <<0, 0>>, index |-> 0] @@
  s1 :> [zxid |-> <<0, 0>>, index |-> 0] @@
  s0 :> [zxid |-> <<0, 0>>, index |-> 0] )
/\ status = (s2 :> ONLINE @@ s1 :> OFFLINE @@ s0 :> ONLINE)
/\ state = (s2 :> FOLLOWING @@ s1 :> LOOKING @@ s0 :> LEADING)
/\ partition = ( s2 :> (s2 :> FALSE @@ s1 :> FALSE @@ s0 :> FALSE) @@
  s1 :> (s2 :> FALSE @@ s1 :> FALSE @@ s0 :> FALSE) @@
  s0 :> (s2 :> FALSE @@ s1 :> FALSE @@ s0 :> FALSE) )
/\ packetsSync = ( s2 :> [notCommitted |-> <<>>, committed |-> <<>>] @@
  s1 :> [notCommitted |-> <<>>, committed |-> <<>>] @@
  s0 :> [notCommitted |-> <<>>, committed |-> <<>>] )
/\ ackldRecv = ( s2 :>
      { [sid |-> s2, connected |-> TRUE],
        [sid |-> s1, connected |-> TRUE],
        [sid |-> s0, connected |-> TRUE] } @@
  s1 :> {[sid |-> s1, connected |-> TRUE]} @@
  s0 :> {[sid |-> s0, connected |-> TRUE]} )
/\ servingState = (s2 :> SHUTDOWN @@ s1 :> SHUTDOWN @@ s0 :> SHUTDOWN)
/\ committedLog = << [zxid |-> <<1, 1>>, value |-> 101, ackSid |-> {s2, s1, s0}, epoch |-> 1],
   [zxid |-> <<1, 2>>, value |-> 102, ackSid |-> {s2, s1}, epoch |-> 1] >>
/\ epochLeader = <<{s2}, {s1}, {s0}, {}, {}, {}, {}, {}, {}, {}>>


STATE_17 == 
/\ connectInfo = ( s2 :> [sid |-> s0, syncMode |-> NONE, nlRcv |-> FALSE] @@
  s1 :> [nlRcv |-> FALSE, syncMode |-> NONE, sid |-> NullPoint] @@
  s0 :> [nlRcv |-> FALSE, syncMode |-> NONE, sid |-> NullPoint] )
/\ forwarding = (s2 :> {} @@ s1 :> {} @@ s0 :> {s2})
/\ history = ( s2 :>
      << [ zxid |-> <<1, 1>>,
           value |-> 101,
           ackSid |-> {s2, s1, s0},
           epoch |-> 1 ],
         [ zxid |-> <<1, 2>>,
           value |-> 102,
           ackSid |-> {s2, s1},
           epoch |-> 1 ] >> @@
  s1 :>
      << [zxid |-> <<1, 1>>, value |-> 101, ackSid |-> {s1, s0}, epoch |-> 1],
