/\ recorder = [ step |-> 18,
  nPartition |-> 0,
  nConsequentFailure |-> 0,
  noExecute |-> {<<"NodeCrash", s1>>},
  nTimeout |-> 2,
  nCrash |-> 3,
  nTransaction |-> 2,
  maxEpoch |-> 3,
