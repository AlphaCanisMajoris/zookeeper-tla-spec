  s0 :> [index |-> 1, zxid |-> <<1, 1>>] )
/\ rcvBuffer = ( s2 :> (s2 :> <<>> @@ s1 :> <<>> @@ s0 :> <<>>) @@
  s1 :> (s2 :> <<>> @@ s1 :> <<>> @@ s0 :> <<>>) @@
  s0 :>
      ( s2 :>
            << [mtype |-> TRUNC, mzxid |-> <<1, 1>>],
               [mtype |-> NEWLEADER, mzxid |-> <<3, 0>>] >> @@
        s1 :> <<>> @@
        s0 :> <<>> ) )
/\ initialHistory = ( s2 :>
      << [ zxid |-> <<1, 1>>,
           value |-> 101,
           ackSid |-> {s2, s1, s0},
           epoch |-> 1 ],
         [ zxid |-> <<1, 2>>,
           value |-> 102,
           ackSid |-> {s2, s1},
           epoch |-> 1 ] >> @@
