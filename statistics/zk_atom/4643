/\ tempMaxEpoch = (s2 :> 5 @@ s1 :> 2 @@ s0 :> 3)
/\ leaderOracle = NullPoint
/\ lastSnapshot = ( s2 :> [index |-> 0, zxid |-> <<0, 0>>] @@
  s1 :> [zxid |-> <<0, 0>>, index |-> 0] @@
  s0 :> [index |-> 1, zxid |-> <<1, 1>>] )
/\ status = (s2 :> ONLINE @@ s1 :> ONLINE @@ s0 :> ONLINE)
