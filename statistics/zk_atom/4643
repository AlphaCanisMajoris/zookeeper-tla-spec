/\ rcvBuffer = ( s2 :> (s2 :> <<>> @@ s1 :> <<>> @@ s0 :> <<>>) @@
  s1 :> (s2 :> <<>> @@ s1 :> <<>> @@ s0 :> <<>>) @@
  s0 :>
      ( s2 :> <<>> @@
        s1 :> <<[mtype |-> UPTODATE, mzxid |-> <<6, 0>>]>> @@
        s0 :> <<>> ) )
/\ initialHistory = ( s2 :>
      << [ zxid |-> <<1, 1>>,
           value |-> 101,
           ackSid |-> {s2, s0},
           epoch |-> 1 ] >> @@
  s1 :>
      << [ zxid |-> <<1, 1>>,
           value |-> 101,
           ackSid |-> {s1, s0},
           epoch |-> 1 ] >> @@
  s0 :> <<[zxid |-> <<1, 1>>, value |-> 101, ackSid |-> {s0}, epoch |-> 1]>> )
/\ recorder = [ step |-> 37,
  nPartition |-> 3,
  nConsequentFailure |-> 0,
  noExecute |-> {},
  nTimeout |-> 5,
  nCrash |-> 3,
  nTransaction |-> 2,
  maxEpoch |-> 6,
  pc |-> <<"LeaderProcessACK", s0, s1, <<6, 0>>>>,
  nClientRequest |-> 2 ]
/\ acceptedEpoch = (s2 :> 5 @@ s1 :> 6 @@ s0 :> 6)
/\ connecting = ( s2 :> {[sid |-> s2, connected |-> TRUE], [sid |-> s0, connected |-> TRUE]} @@
  s1 :> {[sid |-> s1, connected |-> TRUE], [sid |-> s0, connected |-> TRUE]} @@
  s0 :> {[sid |-> s1, connected |-> TRUE], [sid |-> s0, connected |-> TRUE]} )
/\ proposalMsgsLog = { [zxid |-> <<1, 1>>, epoch |-> 1, source |-> s2, data |-> 101],
  [zxid |-> <<1, 2>>, epoch |-> 1, source |-> s2, data |-> 102],
  [zxid |-> <<1, 2>>, epoch |-> 2, source |-> s1, data |-> 102] }
/\ hzxid = (s2 :> <<1, 1>> @@ s1 :> <<1, 2>> @@ s0 :> <<6, 0>>)
/\ lastCommitted = ( s2 :> [index |-> 0, zxid |-> <<0, 0>>] @@
  s1 :> [index |-> 0, zxid |-> <<0, 0>>] @@
  s0 :> [index |-> 1, zxid |-> <<1, 1>>] )
/\ daInv = [ messageLegal |-> TRUE,
  commitConsistent |-> TRUE,
  stateConsistent |-> TRUE,
  proposalConsistent |-> TRUE,
  ackConsistent |-> TRUE ]
/\ currentEpoch = (s2 :> 5 @@ s1 :> 6 @@ s0 :> 6)
/\ learners = (s2 :> {} @@ s1 :> {} @@ s0 :> {s1, s0})
/\ tempMaxEpoch = (s2 :> 5 @@ s1 :> 2 @@ s0 :> 6)
/\ leaderOracle = s0
/\ lastSnapshot = ( s2 :> [index |-> 0, zxid |-> <<0, 0>>] @@
  s1 :> [index |-> 0, zxid |-> <<0, 0>>] @@
  s0 :> [index |-> 1, zxid |-> <<1, 1>>] )
/\ status = (s2 :> ONLINE @@ s1 :> ONLINE @@ s0 :> ONLINE)
/\ state = (s2 :> LOOKING @@ s1 :> FOLLOWING @@ s0 :> LEADING)
/\ partition = ( s2 :> (s2 :> FALSE @@ s1 :> FALSE @@ s0 :> FALSE) @@
  s1 :> (s2 :> FALSE @@ s1 :> FALSE @@ s0 :> FALSE) @@
  s0 :> (s2 :> FALSE @@ s1 :> FALSE @@ s0 :> FALSE) )
/\ packetsSync = ( s2 :> [notCommitted |-> <<>>, committed |-> <<>>] @@
  s1 :> [notCommitted |-> <<>>, committed |-> <<>>] @@
  s0 :> [notCommitted |-> <<>>, committed |-> <<>>] )
/\ ackldRecv = ( s2 :> {[sid |-> s2, connected |-> TRUE]} @@
  s1 :> {[sid |-> s1, connected |-> TRUE]} @@
  s0 :> {[sid |-> s1, connected |-> TRUE], [sid |-> s0, connected |-> TRUE]} )
/\ servingState = (s2 :> SHUTDOWN @@ s1 :> INITIAL @@ s0 :> RUNNING)
/\ committedLog = <<[zxid |-> <<1, 1>>, value |-> 101, ackSid |-> {s1, s0}, epoch |-> 1]>>
