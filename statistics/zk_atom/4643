  s1 :> [index |-> 2, zxid |-> <<1, 2>>] @@
  s0 :> [index |-> 1, zxid |-> <<1, 1>>] )
/\ rcvBuffer = ( s2 :> (s2 :> <<>> @@ s1 :> <<>> @@ s0 :> <<>>) @@
  s1 :> (s2 :> <<>> @@ s1 :> <<>> @@ s0 :> <<>>) @@
  s0 :> (s2 :> <<>> @@ s1 :> <<>> @@ s0 :> <<>>) )
